{
  "name": "Quick launch",
  "description": "Press Ctrl+Shift+F",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
  	"storage",
    "tabs",
    "notifications"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "browser_action.html"
  },
  "icons": { 
  	"16": "icons/icon16.png",    
    "48": "icons/icon48.png",     
    "128": "icons/icon128.png" 
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  }
}
